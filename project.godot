; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "Game",
"language": "GDScript",
"path": "res://Game/Game.gd"
}, {
"base": "Resource",
"class": "GameEvent",
"language": "GDScript",
"path": "res://Game/Events/Event.gd"
}, {
"base": "Node",
"class": "GamePlayer",
"language": "GDScript",
"path": "res://Game/Player.gd"
} ]
_global_script_class_icons={
"Game": "",
"GameEvent": "",
"GamePlayer": ""
}

[application]

config/name="Text based adventure"
run/main_scene="res://Game/Game.tscn"
config/icon="res://icon.png"

[autoload]

DB="*res://Game/Database.gd"
Global="*res://Game/Global.gd"
Resources="*res://Game/Resources.tscn"

[display]

window/size/always_on_top=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 0, 0, 0, 1 )
environment/default_environment="res://default_env.tres"
